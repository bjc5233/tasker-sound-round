    &lt;!-- Initialize Swiper --&gt;
    &lt;script&gt;
	var isPlaying = false;
	var scenes = [
		%scene_confs
	];
	for (var i=0; i &lt; scenes.length; i++) {
		var scene = scenes[i];
		$(".swiper-wrapper").append("&lt;div class='swiper-slide'&gt;&lt;canvas id='scene" 
									+ i + "' width='280' height='280' onclick='playSceneSound(\"" 
									+ scene.sound + "\")'&gt;&lt;/div&gt;");
		drawScene("scene" + i, scene.name);
	}
	function drawScene(scene, name) {
		var bg = document.getElementById(scene);
		var ctx = bg.getContext('2d');
		ctx.beginPath();
		ctx.arc(140, 140, 130, 0*Math.PI, 2*Math.PI, false);
		ctx.lineWidth = 5;
		ctx.strokeStyle = 'rgba(255, 255, 255, 0.4)';
		ctx.stroke();
		ctx.font = "40px SimSun";
		ctx.fillStyle = "#FFFFFF";
		ctx.textAlign="center";
		ctx.fillText(name, 140, 155);
	}

    var swiper = new Swiper('.swiper-container', {
		direction: 'horizontal',
        pagination: '.swiper-pagination',
        paginationClickable: false,
		onSlideChangeStart: function(swiper){
		}
    });
	function playSceneSound(sound) {
		if (isPlaying) {
			swiper.enableTouchControl();
			musicStop();
			isPlaying = false;
		} else {
			swiper.disableTouchControl();
			performTask("sound-round-play", 5, "%SOUND_ROUND_ROOT_PATH/sound/" + sound);
			isPlaying = true;
		}
	}
    &lt;/script&gt;
&lt;/body&gt;
&lt;/html&gt;</Str>
