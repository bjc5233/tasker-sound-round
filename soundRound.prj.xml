&lt;html lang="en"&gt;
&lt;head&gt;
    &lt;meta charset="utf-8"&gt;
    &lt;title&gt;Swiper demo&lt;/title&gt;
    &lt;meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1"&gt;

    &lt;!-- Link Swiper's CSS --&gt;
    &lt;link rel="stylesheet" href="%SOUND_ROUND_ROOT_PATH/swiper-3.4.2.min.css"&gt;

    &lt;!-- Demo styles --&gt;
    &lt;style&gt;
    html, body {
        position: relative;
        height: 100%;
    }
    body {
        font-family: Helvetica;
        color:#000;
        margin: 0;
        padding: 0;
    }
    .swiper-container {
        width: 100%;
        height: 100%;
    }
    .swiper-slide {
        text-align: center;

        /* Center slide text vertically */
        display: -webkit-box;
        display: -ms-flexbox;
        display: -webkit-flex;
        display: flex;
        -webkit-box-pack: center;
        -ms-flex-pack: center;
        -webkit-justify-content: center;
        justify-content: center;
        -webkit-box-align: center;
        -ms-flex-align: center;
        -webkit-align-items: center;
        align-items: center;
    }
    &lt;/style&gt;
&lt;/head&gt;
&lt;body&gt;
    &lt;!-- Swiper --&gt;
    &lt;div class="swiper-container"&gt;
        &lt;div class="swiper-wrapper"&gt;
        &lt;/div&gt;
        &lt;!-- Add Pagination --&gt;
        &lt;div class="swiper-pagination"&gt;&lt;/div&gt;
    &lt;/div&gt;

    &lt;!-- Swiper JS --&gt;
	&lt;script type="text/javascript" src="%SOUND_ROUND_ROOT_PATH/jquery-3.2.1.min.js"&gt;&lt;/script&gt;
